{
    "kind": "Pod",
    "apiVersion": "v1",
    "metadata": {
        "name": "nginx-test-b",
        "labels": {
            "name": "service-nginx"
        }
    },
    "spec": {
        "containers": [
            {
                "name": "nginx",
                "image": "nginx:latest",
                "ports": [
                    {
                        "containerPort": 80,
                        "hostPort": 8089
                    }
                ]
            }
        ]
    }
}